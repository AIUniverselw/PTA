#include<stdio.h>

int main ()
{
	double open, high, low, close;
	int sh, so;
	char shadow[4][50] = {" with Lower Shadow and Upper Shadow", " with Lower Shadow",
	" with Upper Shadow", ""};
	char solid[3][20] = {"BW-Solid", "R-Hollow", "R-Cross"};
	scanf("%lf %lf %lf %lf", &open, &high, &low, &close);
	if (low < open && low < close && high > open && high > close)
		sh = 0;
	else if (low < open && low < close)
		sh = 1;
	else if (high > open && high > close)
		sh = 2;
	else
		sh = 3;
	
	if (close < open)
		so = 0;
	else if (close > open)
		so = 1;
	else if (close = open)
		so = 2;
	printf("%s%s", solid[so], shadow[sh]);
	return 0;
}
